{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hl7",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "msh"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "segment"
              },
              {
                "type": "PATTERN",
                "value": "\\r|\\r\\n|\\n"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "segment"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "field_separator": {
      "type": "STRING",
      "value": "|"
    },
    "msh": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "MSH"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "STRING",
          "value": "^~\\&"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "field"
              }
            ]
          }
        },
        {
          "type": "PATTERN",
          "value": "\\r|\\r\\n|\\n"
        }
      ]
    },
    "segment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "segment_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "field"
              }
            ]
          }
        }
      ]
    },
    "segment_name": {
      "type": "PATTERN",
      "value": "[A-Z]{1}[A-Z0-9]{2}"
    },
    "field": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "~"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "repeat"
        }
      ]
    },
    "repeat": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "^"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "component"
        }
      ]
    },
    "component": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "&"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "subcomponent"
        }
      ]
    },
    "subcomponent": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        }
      ]
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "PATTERN",
          "value": "0[xX][0-9a-fA-F]+"
        }
      ]
    },
    "float": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d*\\.\\d+"
        },
        {
          "type": "PATTERN",
          "value": "\\d+\\.\\d*"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^|\\r\\n]*"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^\"]"
                    },
                    {
                      "type": "STRING",
                      "value": "\"\""
                    }
                  ]
                }
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}